{
  "manifest_version": 3,
  "name": "uploadFile",
  "version": "1.0",
  "description": "上传图片到COS",
  "author": "lihuaxin",
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "default_icon": {
    "16": "/assets/avatar.png",
    "32": "/assets/avatar.png",
    "48": "/assets/avatar.png",
    "128": "/assets/avatar.png"
  }
}
